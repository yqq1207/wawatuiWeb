.demos-content-padded{
	padding: 15px;
}
.money-head{
	width: 110px;
	height: 110px;
	margin: 60px auto 20px;
}
.money-head img{
	width: 110px;
	height: 110px;
}
.money-all{
	display: flex;
	display: -webkit-flex;
	justify-content:center;
	align-items:center;
}
.money-all img{
	/*width: 28px;
	height: 28px;*/
	width: 34px;
	height: 34px;
	padding-right: 3px;
	vertical-align: middle;
}
.money-detial{
	display: block;
	width: 100%;
	position: fixed;
	bottom: 30px;
	left: 0;
}
.weui-popup__modal{
	width: 100%;
	box-sizing: border-box;
}
.weui-popup__container--visible .weui-popup__overlay{
	opacity: 0.9;
	background-color:rgba(0,0,0,0.8)
}